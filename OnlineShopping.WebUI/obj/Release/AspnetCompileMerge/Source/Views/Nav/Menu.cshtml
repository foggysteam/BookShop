@model IEnumerable<string>

@*@Html.ActionLink("All", "List", "Product", null, new { @class = "btn btn-block btn-default btn-lg" })*@
@Html.ActionLink("All", "List", "Product", null, new { @class = "resp-tab-item" })



@foreach (var link in Model)
{
    @Html.RouteLink(link, new
{
    controller = "Product",
    action = "List",
    category = link,
    page = 1
},
        //new { @class = "btn btn-block btn-default btn-lg"
        new { @class = "resp-tab-item"
        + (link == ViewBag.SelectedCategory? " btn-success" : "") }
    )
}
